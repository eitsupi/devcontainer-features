{
	"jq-17rc": {
		"image": "debian:stable-slim",
		"features": {
			"jq-likes": {
				"jqVersion": "1.7rc1",
				"allowJqRcVersion": true
			}
		}
	},
	"jq-latest-allow-rc": {
		"image": "debian:stable-slim",
		"features": {
			"jq-likes": {
				"jqVersion": "latest",
				"allowJqRcVersion": true
			}
		}
	},
	"yq-latest": {
		"image": "debian:stable-slim",
		"features": {
			"jq-likes": {
				"jqVersion": "none",
				"yqVersion": "latest"
			}
		}
	},
	"gojq-latest": {
		"image": "debian:stable-slim",
		"features": {
			"jq-likes": {
				"jqVersion": "none",
				"gojqVersion": "latest"
			}
		}
	},
	"xq-latest": {
		"image": "debian:stable-slim",
		"features": {
			"jq-likes": {
				"jqVersion": "none",
				"xqVersion": "latest"
			}
		}
	},
	"install-with-zsh-fish-pwsh-non-root": {
		"image": "mcr.microsoft.com/devcontainers/base:debian",
		"features": {
			"ghcr.io/meaningful-ooo/devcontainer-features/fish": {},
			"ghcr.io/devcontainers/features/powershell": {},
			"jq-likes": {
				"jqVersion": "none",
				"yqVersion": "latest",
				"gojqVersion": "latest"
			}
		}
	},
	"install-with-fish-pwsh-root": {
		"image": "debian:stable-slim",
		"features": {
			"ghcr.io/meaningful-ooo/devcontainer-features/fish": {},
			"ghcr.io/devcontainers/features/powershell": {},
			"jq-likes": {
				"jqVersion": "none",
				"yqVersion": "latest",
				"gojqVersion": "latest"
			}
		}
	},
	"yq-gojq-xq-specific-version": {
		"image": "debian:stable-slim",
		"features": {
			"jq-likes": {
				"jqVersion": "none",
				"yqVersion": "4.24",
				"gojqVersion": "0.7",
				"xqVersion": "0.2"
			}
		}
	}
}
